{
  "kind": "collectionType",
  "collectionName": "issue_reports",
  "info": {
    "singularName": "issue-report",
    "pluralName": "issue-reports",
    "displayName": "issue-report"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "integer"
    },
    "place_slug": {
      "type": "string"
    },
    "place_name": {
      "type": "string"
    },
    "issue_type": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "admin_notes": {
      "type": "string"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "issue_reports"
    },
    "resolved_at": {
      "type": "datetime"
    },
    "feedback_type": {
      "type": "enumeration",
      "enum": [
        "issue",
        "feature"
      ]
    },
    "statusReport": {
      "type": "enumeration",
      "enum": [
        "open",
        "in_progress",
        "resolved",
        "closed"
      ]
    }
  }
}
