{
  "kind": "collectionType",
  "collectionName": "amenities",
  "info": {
    "singularName": "amenity",
    "pluralName": "amenities",
    "displayName": "Amenity"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid"
    },
    "description": {
      "type": "string"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "places": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::place.place",
      "mappedBy": "amenities"
    }
  }
}
